{
    "name": "tienvx/mbt-bundle",
    "type": "symfony-bundle",
    "description": "Core Library For SICOPE Model Project",
    "keywords": ["model based testing", "testing tools"],
    "license": "MIT",
    "authors": [
        {
          "name": "Tien Xuan Vo",
          "homepage": "https://tienvx.github.io/",
          "role": "Maintainer & Developer"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/tienvx/mbt-bundle/contributors"
        }
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/orm": "^2.8",
        "egulias/email-validator": "^2.1",
        "jmgq/a-star": "^1.1",
        "league/flysystem": "^2.0",
        "php-webdriver/webdriver": "^1.9",
        "symfony/config": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/expression-language": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/messenger": "^5.2",
        "symfony/mime": "^5.2",
        "symfony/notifier": "^5.2",
        "symfony/security-core": "^5.2",
        "symfony/translation": "^5.2",
        "symfony/validator": "^5.2",
        "tienvx/single-color-petrinet": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Tienvx\\Bundle\\MbtBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tienvx\\Bundle\\MbtBundle\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "suggest": {
        "ext-amqp": "Needed to support amqp transport for messenger component",
        "symfony/mailer": "Support notifying bug using email",
        "symfony/twig-pack": "Support notifying bug using email",
        "twig/cssinliner-extra": "Support notifying bug using email",
        "twig/inky-extra": "Support notifying bug using email",
        "symfony/nexmo-notifier": "Notify bug using Nexmo API",
        "symfony/slack-notifier": "Notify bug to Slack",
        "symfony/telegram-notifier": "Notify bug to Telegram",
        "symfony/twilio-notifier": "Notify bug using Twilio API"
    }
}
